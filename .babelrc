// babel compiler configuration file
// https://babeljs.io/docs/usage/babelrc

{
    "presets": [
        "es2015",
        "react"
    ],
    "plugins": [
        ["babel-root-slash-import", {
            "rootPathSuffix": "src"
        }],
        ["babel-plugin-webpack-alias", {
            "config": "./webpack.config.js"
        }]
    ]
}
